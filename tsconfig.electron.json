{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "dist-electron",
        "rootDir": "electron",
        "allowJs": true,
        "types": ["node", "electron"]
    },
    "include": ["electron/**/*"]
}

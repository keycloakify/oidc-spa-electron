{
    "name": "example-react-router",
    "private": true,
    "version": "0.0.0",
    "main": "dist-electron/main.js",
    "type": "module",
    "scripts": {
        "prepare": "[ ! -f .env.local ] && cp .env.local.sample .env.local || true",
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "build:electron": "tsc -p tsconfig.electron.json",
        "dev:electron": "yarn build:electron && concurrently \"rm -rf node_modules/.vite && yarn dev\" \"wait-on http://localhost:5173 && electron .\""
    },
    "dependencies": {
        "oidc-spa": "latest",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-router": "^7.4.0",
        "zod": "3.24.2"
    },
    "devDependencies": {
        "@types/react": "19.0.12",
        "@types/react-dom": "19.0.4",
        "@types/node": "22.13.11",
        "@typescript-eslint/eslint-plugin": "8.27.0",
        "@typescript-eslint/parser": "8.27.0",
        "@vitejs/plugin-react": "4.3.4",
        "eslint": "9.23.0",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-react-refresh": "0.4.19",
        "typescript": "5.7.2",
        "vite": "6.2.2",
        "vite-plugin-commonjs": "0.10.4",
        "electron": "^28.1.0",
        "concurrently": "^8.2.2",
        "wait-on": "^7.2.0"
    }
}
